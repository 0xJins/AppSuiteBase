<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:sys="using:System">

	<!-- AppCulture -->
	<sys:String x:Key="String/ApplicationCulture.System">系統設定</sys:String>
	
	<!-- Common -->
	<sys:String x:Key="String/Common.Add">加入</sys:String>
	<sys:String x:Key="String/Common.Cancel">取消</sys:String>
	<sys:String x:Key="String/Common.Close">關閉</sys:String>
	<sys:String x:Key="String/Common.Copy">複製</sys:String>
	<sys:String x:Key="String/Common.Create">建立</sys:String>
	<sys:String x:Key="String/Common.Delete">刪除</sys:String>
	<sys:String x:Key="String/Common.Edit">編輯</sys:String>
	<sys:String x:Key="String/Common.Empty">空</sys:String>
	<sys:String x:Key="String/Common.Load">載入</sys:String>
	<sys:String x:Key="String/Common.No">否</sys:String>
	<sys:String x:Key="String/Common.None">無</sys:String>
	<sys:String x:Key="String/Common.OK">確定</sys:String>
	<sys:String x:Key="String/Common.Open">開啟</sys:String>
	<sys:String x:Key="String/Common.Paste">貼上</sys:String>
	<sys:String x:Key="String/Common.Remove">移除</sys:String>
	<sys:String x:Key="String/Common.Save">儲存</sys:String>
	<sys:String x:Key="String/Common.Yes">是</sys:String>

	<!-- FallbackFontFamilies -->
	<sys:String x:Key="String/TextBox.FallbackFontFamilies">Microsoft JhengHei,蘋方-繁</sys:String>

	<!-- ThemeMode -->
	<sys:String x:Key="String/ThemeMode.Dark">深色</sys:String>
	<sys:String x:Key="String/ThemeMode.Light">淺色</sys:String>
	<sys:String x:Key="String/ThemeMode.System">系統設定</sys:String>

	<!-- TimeSpanConverter -->
	<sys:String x:Key="String/TimeSpanConverter.Days">{0} 天 {1} 小時 {2} 分 {3}.{4:000} 秒</sys:String>
	<sys:String x:Key="String/TimeSpanConverter.Hours">{0} 小時 {1} 分 {2}.{3:000} 秒</sys:String>
	<sys:String x:Key="String/TimeSpanConverter.Milliseconds">{0} 毫秒</sys:String>
	<sys:String x:Key="String/TimeSpanConverter.Minutes">{0} 分 {1}.{2:000} 秒</sys:String>
	<sys:String x:Key="String/TimeSpanConverter.Seconds">{0}.{1:000} 秒</sys:String>
	
</ResourceDictionary>
