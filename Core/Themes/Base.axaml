		<Color x:Key="Color/SplashWindow.Foreground">#ffffff</Color>
		<CornerRadius x:Key="CornerRadius/SplashWindow">8</CornerRadius>
		<sys:Double x:Key="Double/SplashWindow.Icon.Size">80</sys:Double>
		<sys:Double x:Key="Double/SplashWindow.Message.FontSize">12</sys:Double>
		<sys:Double x:Key="Double/SplashWindow.Message.ReservedSpace">50</sys:Double>
		<sys:Double x:Key="Double/SplashWindow.Title.FontSize">30</sys:Double>
		<Thickness x:Key="Thickness/SplashWindow.Border">1</Thickness>
		<Thickness x:Key="Thickness/SplashWindow.Icon.Margin">0,0,10,0</Thickness>
		<Thickness x:Key="Thickness/SplashWindow.Message.Margin">0,30,0,0</Thickness>
		<Thickness x:Key="Thickness/SplashWindow.Padding">20</Thickness>
		<Thickness x:Key="Thickness/SplashWindow.Title.Margin">0,0,0,5</Thickness>

		<!-- StatusBar -->
		<Color x:Key="Color/StatusBar.Background.Inactive">#ffffff</Color>
		<Color x:Key="Color/StatusBar.Button.Background.PointerOver">#3fffffff</Color>
		<Color x:Key="Color/StatusBar.Button.Background.Pressed">#3f000000</Color>
		<Color x:Key="Color/StatusBar.Foreground">#ffffff</Color>
		<Color x:Key="Color/StatusBar.Foreground.Disabled">#7fffffff</Color>
		<Color x:Key="Color/StatusBar.ToggleButton.Background.Checked">#7f000000</Color>
		<sys:Double x:Key="Double/StatusBar.DropDownIcon.Size">8</sys:Double>
		<sys:Double x:Key="Double/StatusBar.Icon.Size">16</sys:Double>
		<SolidColorBrush x:Key="Brush/StatusBar.Background" Color="{DynamicResource SystemAccentColorDark1}"/>
		<SolidColorBrush x:Key="Brush/StatusBar.Background.Inactive" Color="{DynamicResource Color/StatusBar.Background.Inactive}"/>
		<SolidColorBrush x:Key="Brush/StatusBar.Button.Background.PointerOver" Color="{DynamicResource Color/StatusBar.Button.Background.PointerOver}"/>
		<SolidColorBrush x:Key="Brush/StatusBar.Button.Background.Pressed" Color="{DynamicResource Color/StatusBar.Button.Background.Pressed}"/>
		<SolidColorBrush x:Key="Brush/StatusBar.Foreground" Color="{DynamicResource Color/StatusBar.Foreground}"/>
		<SolidColorBrush x:Key="Brush/StatusBar.Foreground.Disabled" Color="{DynamicResource Color/StatusBar.Foreground.Disabled}"/>
		<SolidColorBrush x:Key="Brush/StatusBar.ToggleButton.Background.Checked" Color="{DynamicResource Color/StatusBar.ToggleButton.Background.Checked}"/>
		<Thickness x:Key="Thickness/StatusBar.Button.Padding">5</Thickness>
		<Thickness x:Key="Thickness/StatusBar.DropDownIcon.Margin">8,0,0,0</Thickness>
		<Thickness x:Key="Thickness/StatusBar.Icon.Margin">2,0,2,0</Thickness>
		<Thickness x:Key="Thickness/StatusBar.Item.Margin">1,5,1,5</Thickness>
		<Thickness x:Key="Thickness/StatusBar.Padding">5,0,5,0</Thickness>

		<!-- TabControl -->
		<sys:Double x:Key="Double/TabControl.TabStrip.ExtendedClientAreaReserveSpace">20</sys:Double>
		<Thickness x:Key="Thickness/TabControl.TabStrip.Padding">6,0,6,0</Thickness>

		<!-- TabItem -->
		<SolidColorBrush x:Key="TabItemHeaderSelectedPipeFill">#00000000</SolidColorBrush>
		<SolidColorBrush x:Key="Brush/TabItem.Header.Background" Color="{DynamicResource Color/TabItem.Header.Background}"/>
		<SolidColorBrush x:Key="Brush/TabItem.Header.Background.PointerOver" Color="{DynamicResource Color/TabItem.Header.Background.PointerOver}"/>
		<SolidColorBrush x:Key="Brush/TabItem.Header.Background.Selected" Color="{DynamicResource Color/TabItem.Header.Background.Selected}"/>
		<SolidColorBrush x:Key="Brush/TabItem.Header.Border" Color="{DynamicResource Color/TabItem.Header.Border}"/>
		<SolidColorBrush x:Key="Brush/TabItem.Header.Border.PointerOver" Color="{DynamicResource Color/TabItem.Header.Border.PointerOver}"/>
		<SolidColorBrush x:Key="Brush/TabItem.Header.Border.Selected" Color="{DynamicResource Color/TabItem.Header.Border.Selected}"/>
		<Color x:Key="Color/TabItem.Header.Background">#ffffff</Color>
		<Color x:Key="Color/TabItem.Header.Background.PointerOver">#ffffff</Color>
		<Color x:Key="Color/TabItem.Header.Background.Selected">#ffffff</Color>
		<Color x:Key="Color/TabItem.Header.Border">#ffffff</Color>
		<Color x:Key="Color/TabItem.Header.Border.PointerOver">#ffffff</Color>
		<Color x:Key="Color/TabItem.Header.Border.Selected">#ffffff</Color>
		<CornerRadius x:Key="CornerRadius/TabItem.Header">5,5,0,0</CornerRadius>
		<sys:Double x:Key="Double/TabItem.Header.ColorBar.Height">3</sys:Double>
		<sys:Double x:Key="Double/TabItem.Header.Content.Height">20</sys:Double>
		<sys:Double x:Key="Double/TabItem.Header.FontSize">14</sys:Double>
		<sys:Double x:Key="Double/TabItem.Header.Height">40</sys:Double>
		<sys:Double x:Key="Double/TabItem.Header.Icon.Size">16</sys:Double>
		<sys:Double x:Key="Double/TabItem.Header.MaxWidth">250</sys:Double>
		<Thickness x:Key="Thickness/TabItem.Header.Border">0,0,1,0</Thickness>
		<Thickness x:Key="Thickness/TabItem.Header.Button.Margin">10,0,0,0</Thickness>
		<Thickness x:Key="Thickness/TabItem.Header.Button.Padding">3</Thickness>
		<Thickness x:Key="Thickness/TabItem.Header.Icon.Margin">0,0,10,0</Thickness>
		<Thickness x:Key="Thickness/TabItem.Header.Margin">1,2,1,0</Thickness>
		<Thickness x:Key="Thickness/TabItem.Header.Padding">10</Thickness>

		<!-- TextBox -->
		<Thickness x:Key="TextControlBorderThemeThicknessFocused">0,0,0,2</Thickness>

		<!-- TextInputDialog -->
		<sys:Double x:Key="Double/TextInputDialog.Width">450</sys:Double>

		<!-- ToolBar -->
		<Color x:Key="Color/ToolBar.Background">#ffffff</Color>
		<sys:Double x:Key="Double/ToolBar.Button.Size">34</sys:Double>
		<SolidColorBrush x:Key="Brush/ToolBar.Background" Color="{DynamicResource Color/ToolBar.Background}"/>
		<Thickness x:Key="Thickness/ToolBar.Button.Padding">5</Thickness>
		<Thickness x:Key="Thickness/ToolBar.Item.Margin">1,0,1,0</Thickness>
		<Thickness x:Key="Thickness/ToolBar.Padding">3</Thickness>
		<Thickness x:Key="Thickness/ToolBar.Separator.Margin">2,3,2,3</Thickness>
		<Thickness x:Key="Thickness/ToolBar.TextBlock.Margin">3,0,2,0</Thickness>

		<!-- Window -->
		<SolidColorBrush x:Key="Brush/Window.Background" Color="{DynamicResource Color/Window.Background}"/>
		<SolidColorBrush x:Key="Brush/Window.Background.Tabbed" Color="{DynamicResource Color/Window.Background.Tabbed}"/>
		<SolidColorBrush x:Key="Brush/Window.Background.Tabbed.Transparent" Color="{DynamicResource Color/Window.Background.Tabbed.Transparent}"/>
		<SolidColorBrush x:Key="Brush/Window.Background.Transparent" Color="{DynamicResource Color/Window.Background.Transparent}"/>
		<Color x:Key="Color/Window.Background">#ffffff</Color>
		<Color x:Key="Color/Window.Background.Tabbed">#ffffff</Color>
		<Color x:Key="Color/Window.Background.Tabbed.Transparent">#ffffff</Color>
		<Color x:Key="Color/Window.Background.Transparent">#ffffff</Color>

		<!-- WorkingArea -->
		<SolidColorBrush x:Key="Brush/WorkingArea.Background" Color="{DynamicResource Color/WorkingArea.Background}"/>
		<SolidColorBrush x:Key="Brush/WorkingArea.Panel.Background" Color="{DynamicResource Color/WorkingArea.Panel.Background}"/>
		<Color x:Key="Color/WorkingArea.Background">#ffffff</Color>
		<Color x:Key="Color/WorkingArea.Panel.Background">#ffffff</Color>
		
	</Styles.Resources>

	<!-- AddTabItemButton -->
	<Style Selector="Border.AddTabItemButton">
		<Setter Property="Background" Value="{DynamicResource Brush/TabItem.Header.Background.Selected}"/>
		<Setter Property="CornerRadius" Value="{DynamicResource ControlCornerRadius}"/>
		<Setter Property="Height" Value="{DynamicResource Double/AddTabItemButton.Size}"/>
		<Setter Property="HorizontalAlignment" Value="Center"/>
		<Setter Property="Opacity" Value="0.7"/>
		<Setter Property="Padding" Value="{DynamicResource Thickness/AddTabItemButton.Padding}"/>
		<Setter Property="VerticalAlignment" Value="Center"/>
		<Setter Property="Width" Value="{DynamicResource Double/AddTabItemButton.Size}"/>
	</Style>
	<Style Selector="Border.AddTabItemButton:pointerover">
		<Setter Property="Opacity" Value="1"/>
	</Style>

	<!-- BrowseButton -->
	<Style Selector="Button.BrowseButton">
		<Setter Property="Content" Value="…"/>
		<Setter Property="Margin" Value="{DynamicResource Thickness/BrowseButton.Margin}"/>
		<Setter Property="VerticalAlignment" Value="Stretch"/>
	</Style>
	<Style Selector="ToggleButton.BrowseButton">
		<Setter Property="Content" Value="…"/>
		<Setter Property="Margin" Value="{DynamicResource Thickness/BrowseButton.Margin}"/>
		<Setter Property="VerticalAlignment" Value="Stretch"/>
	</Style>

	<!-- ComboBox -->
	<Style Selector="ComboBox">
		<Setter Property="BorderThickness" Value="0,0,0,1"/>
	</Style>

	<!-- Dialog -->
	<Style Selector="Border.Dialog_ContentPanel">
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
	</Style>
	<Style Selector="Border.Dialog_ControlButtonsPanel">
		<Setter Property="Background" Value="{StaticResource Brush/Dialog.ControlButtonsPanel.Background}"/>
		<Setter Property="DockPanel.Dock" Value="Bottom"/>
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
		<Setter Property="Padding" Value="{DynamicResource Thickness/Dialog.ControlButtonsPanel.Padding}"/>
		<Setter Property="VerticalAlignment" Value="Bottom"/>
	</Style>
	<Style Selector="Border.Dialog_TextBlock_Header1">
		<Setter Property="BorderBrush" Value="{StaticResource Brush/Dialog.TextBlock.Border.Header1}"/>
		<Setter Property="BorderThickness" Value="{DynamicResource Thickness/Dialog.TextBlock.Border.Header1}"/>
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.TextBlock.Margin.Header1}"/>
		<Setter Property="Padding" Value="{DynamicResource Thickness/Dialog.TextBlock.Padding.Header1}"/>
	</Style>
	<Style Selector="Button.Dialog_ControlButton">
		<Setter Property="HorizontalContentAlignment" Value="Center"/>
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.ControlButton.Margin}"/>
		<Setter Property="MinWidth" Value="{DynamicResource Double/Dialog.ControlButton.MinWidth}"/>
		<Setter Property="Padding" Value="{DynamicResource Thickness/Dialog.ControlButton.Padding}"/>
		<Setter Property="VerticalContentAlignment" Value="Center"/>
	</Style>
	<Style Selector="ComboBox.Dialog_ComboBox">
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
	</Style>
	<Style Selector="Grid.Dialog_ContentPanel">
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.ContentPanel.Padding}"/>
	</Style>
	<Style Selector="controls|LinkTextBlock.Dialog_LinkTextBlock_Description">
		<Setter Property="FontSize" Value="{DynamicResource Double/Dialog.TextBlock.FontSize.Description}"/>
		<Setter Property="HorizontalAlignment" Value="Left"/>
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.TextBlock.Margin.Description}"/>
		<Setter Property="TextWrapping" Value="Wrap"/>
	</Style>
	<Style Selector="ListBox.Dialog_ListBox">
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
	</Style>
	<Style Selector="NumericUpDown.Dialog_NumericUpDown">
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
	</Style>
	<Style Selector="Panel.Dialog_ContentPanel">
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.ContentPanel.Padding}"/>
	</Style>
	<Style Selector="ScrollViewer.Dialog_ContentPanel">
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
		<Setter Property="HorizontalScrollBarVisibility" Value="Disabled"/>
		<Setter Property="VerticalAlignment" Value="Stretch"/>
	</Style>
	<Style Selector="Separator.Dialog_ControlButton_Separator">
		<Setter Property="Background" Value="{x:Null}"/>
		<Setter Property="Width" Value="{DynamicResource Double/Dialog.ControlButton.Separator.Width}"/>
	</Style>
	<Style Selector="Separator.Dialog_Separator">
		<Setter Property="Background" Value="{x:Null}"/>
		<Setter Property="Height" Value="{DynamicResource Double/Dialog.Separator.Height}"/>
		<Setter Property="Width" Value="{DynamicResource Double/Dialog.Separator.Width}"/>
	</Style>
	<Style Selector="Separator.Dialog_Separator_Large">
		<Setter Property="Background" Value="{x:Null}"/>
		<Setter Property="Height" Value="{DynamicResource Double/Dialog.Separator.Height.Large}"/>
		<Setter Property="Width" Value="{DynamicResource Double/Dialog.Separator.Width.Large}"/>
	</Style>
	<Style Selector="Separator.Dialog_Separator_Small">
		<Setter Property="Background" Value="{x:Null}"/>
		<Setter Property="Height" Value="{DynamicResource Double/Dialog.Separator.Height.Small}"/>
		<Setter Property="Width" Value="{DynamicResource Double/Dialog.Separator.Width.Small}"/>
	</Style>
	<Style Selector="StackPanel.Dialog_ContentPanel">
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.ContentPanel.Padding}"/>
		<Setter Property="Orientation" Value="Vertical"/>
	</Style>
	<Style Selector="StackPanel.Dialog_ControlButtonsPanel">
		<Setter Property="HorizontalAlignment" Value="Center"/>
		<Setter Property="Orientation" Value="Horizontal"/>
	</Style>
	<Style Selector="TextBlock.Dialog_TextBlock">
		<Setter Property="FontSize" Value="{DynamicResource Double/Dialog.TextBlock.FontSize}"/>
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.TextBlock.Margin}"/>
	</Style>
	<Style Selector="TextBlock.Dialog_TextBlock_Description">
		<Setter Property="FontSize" Value="{DynamicResource Double/Dialog.TextBlock.FontSize.Description}"/>
		<Setter Property="Foreground" Value="{StaticResource Brush/Dialog.TextBlock.Foreground.Description}"/>
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.TextBlock.Margin.Description}"/>
		<Setter Property="TextWrapping" Value="Wrap"/>
	</Style>
	<Style Selector="TextBlock.Dialog_TextBlock_Description_Error">
		<Setter Property="FontSize" Value="{DynamicResource Double/Dialog.TextBlock.FontSize.Description}"/>
		<Setter Property="Foreground" Value="{StaticResource Brush/Dialog.TextBlock.Foreground.Error}"/>
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.TextBlock.Margin.Description}"/>
		<Setter Property="TextWrapping" Value="Wrap"/>
	</Style>
	<Style Selector="TextBlock.Dialog_TextBlock_Description_Warning">
		<Setter Property="FontSize" Value="{DynamicResource Double/Dialog.TextBlock.FontSize.Description}"/>
		<Setter Property="Foreground" Value="{StaticResource Brush/Dialog.TextBlock.Foreground.Warning}"/>
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.TextBlock.Margin.Description}"/>
		<Setter Property="TextWrapping" Value="Wrap"/>
	</Style>
	<Style Selector="TextBlock.Dialog_TextBlock_Header1">
		<Setter Property="FontSize" Value="{DynamicResource Double/Dialog.TextBlock.FontSize.Header1}"/>
		<Setter Property="FontWeight" Value="Bold"/>
	</Style>
	<Style Selector="TextBlock.Dialog_TextBlock_Header2">
		<Setter Property="FontSize" Value="{DynamicResource Double/Dialog.TextBlock.FontSize.Header2}"/>
		<Setter Property="Margin" Value="{DynamicResource Thickness/Dialog.TextBlock.Margin.Header2}"/>
	</Style>
	<Style Selector="TextBox.Dialog_TextBox">
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
	</Style>
	<Style Selector="ToggleSwitch.Dialog_ToggleSwitch">
		<Setter Property="Margin" Value="0"/>
	</Style>
	<Style Selector="Window.Dialog">
		<Setter Property="CanResize" Value="False"/>
		<Setter Property="ShowInTaskbar" Value="False"/>
		<Setter Property="SizeToContent" Value="Height"/>
		<Setter Property="WindowStartupLocation" Value="CenterOwner"/>
	</Style>

	<!-- GridSplitter -->
	<Style Selector="GridSplitter.Splitter_Horizontal">
		<Setter Property="VerticalAlignment" Value="Stretch"/>
		<Setter Property="Width" Value="{DynamicResource Thickness/GridSplitter.Thickness}"/>
	</Style>
	<Style Selector="GridSplitter.Splitter_Vertical">
		<Setter Property="Height" Value="{DynamicResource Thickness/GridSplitter.Thickness}"/>
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
	</Style>

	<!-- Icon -->
	<Style Selector="DrawingPresenter.Icon">
		<Setter Property="HorizontalAlignment" Value="Center"/>
		<Setter Property="IsHitTestVisible" Value="False"/>
		<Setter Property="Stretch" Value="Uniform"/>
		<Setter Property="VerticalAlignment" Value="Center"/>
	</Style>
	<Style Selector="DrawingPresenter.Icon:disabled">
		<Setter Property="Opacity" Value="0.3"/>
	</Style>

	<!-- InPlaceAction -->
	<Style Selector="Border.InPlaceAction_Icon">
		<Setter Property="Height" Value="{DynamicResource Double/InPlaceAction.IconSize}"/>
		<Setter Property="Width" Value="{DynamicResource Double/InPlaceAction.IconSize}"/>
	</Style>
	<Style Selector="Border.InPlaceAction_Reserved">
		<Setter Property="Height" Value="{DynamicResource Double/InPlaceAction.ReservedHeight}"/>
		<Setter Property="Opacity" Value="0"/>
