		<!-- MessageDialog -->
		<sys:Double x:Key="Double/MessageDialog.Icon.Size">45</sys:Double>
		<sys:Double x:Key="Double/MessageDialog.MinWidth">200</sys:Double>
		<Thickness x:Key="Thickness/MessageDialog.Icon.Margin">0,0,15,0</Thickness>

		<!-- Separator -->
		<Color x:Key="Color/Separator.Background">#000000</Color>
		<SolidColorBrush x:Key="Brush/Separator.Background" Color="{DynamicResource Color/Separator.Background}"/>

		<!-- SplashWindow -->
		<SolidColorBrush x:Key="Brush/SplashWindow.Background" Color="{DynamicResource Color/SplashWindow.Background}"/>
		<SolidColorBrush x:Key="Brush/SplashWindow.Border" Color="{DynamicResource Color/Accent}"/>
		<SolidColorBrush x:Key="Brush/SplashWindow.Foreground" Color="{DynamicResource Color/SplashWindow.Foreground}"/>
